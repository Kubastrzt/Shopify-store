<div class="products-wrapper">
{% for products in collection.products %}
    {% for settings in section.settings %}
        <img src="{{ section.settings.new-collection | image_url: width: 308, height: 429 }}">
    {% endfor %}
{% endfor %}
</div>
{% schema %}
{
    "name":"Nowości- kolekcja",
    "settings":[{
        "type":"collection",
        "id":"new-collection",
        "label":"Wybierz kolekcje"
    }],
    "presets":[{
        "name":"Nowości- kolekcja",
        "category":"text"
    }]
}
{% endschema %}