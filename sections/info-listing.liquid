{% for block in section.blocks %}
  <img src="{{block.settings.image | img_url: 'medium'}}">
{% endfor %}
{% schema %}
  {
    "name": "Dodaj zdjęcie",
    "blocks":[{
      "type":"image",
      "name":"Wybierz",
        "settings": [
        {
          "type":"image_picker",
          "label":"Twoje zdjecie",
          "id": "image"
        }
      ]
    }],
    "presets":[
    {
      "category":"image",
      "name":"Zdjęcia"
    }]
    
  }
{% endschema %}
